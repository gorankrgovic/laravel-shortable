{
  "name": "gorankrgovic/laravel-shortable",
  "type": "library",
  "description": "Laravel package for creating Youtube-like short IDs.",
  "keywords": [
    "Laravel",
    "eloquent",
    "Youtube",
    "Shorten",
    "Shortable"
  ],
  "homepage": "https://github.com/gorankrgovic/laravel-shortable",
  "license": "MIT",
  "authors": [
    {
      "name": "Goran Krgovic",
      "email": "gorankrgovic1@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "illuminate/config": "~5.7.0",
    "illuminate/database": "~5.7.0",
    "illuminate/support": "~5.7.0"
  },
  "require-dev": {
    "mockery/mockery": "^0.9.4",
    "orchestra/database": "3.7.x-dev",
    "orchestra/testbench": "v3.7.0",
    "phpunit/phpunit": "~7.0",
    "limedeck/phpunit-detailed-printer": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "Gorankrgovic\\LaravelShortable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gorankrgovic\\LaravelShortable\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gorankrgovic\\LaravelShortable\\ServiceProvider"
      ]
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "sort-packages": true
  }
}